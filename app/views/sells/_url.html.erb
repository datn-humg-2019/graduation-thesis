<h2>
  <% if action == "index" %>
    <%= t(".title") %>
  <% elsif action == "new" %>
    <%= link_to t(".title"), bills_path %> > <%= t(".new_title") %>
  <% elsif action == "show" %>
    <%= link_to t(".title"), bills_path %> > <%= t(".edit_title") %>
  <% else %>
    <%= link_to t(".title"), bills_path %> > <%= "select vip user" %>
  <% end %>
</h2>
