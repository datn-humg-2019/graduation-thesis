<div class="col-md-12">
  <div class="card">
    <div class="card-header">
      <%= render "filter" %>
    </div>
    <div class="card-body">
      <div class="">
        <% if params[:type].to_i.zero? %>
          <%= render "today_reports" %>
        <% else %>
          <%= render "report_with" %>
        <% end %>
      </div>
    </div>
    <% if params[:type].to_i.zero? %>
      <div class="card-footer">
        <%= link_to export_reports_inday_path(format: :xlsx),
          class: "btn btn-success float-right btn-margin", target: :_blank do %>
          <i class="fa fa-file-excel-o"></i>Xuáº¥t excell
        <% end %>
      </div>
    <% end %>
  </div>
</div>
